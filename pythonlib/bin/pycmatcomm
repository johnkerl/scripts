from __future__ import division # 1/2 = 0.5, not 0.
import sys
from sackmat_m import *

argc = len(sys.argv)
if (argc != 3):
	print >> sys.stderr, "Usage: %s {file name 1} {file name 2}" % (sys.argv[0])
	sys.exit(1)

A = read_matrix(complex, sys.argv[1])
B = read_matrix(complex, sys.argv[2])
C = A * B - B * A
print_matrix(C)
