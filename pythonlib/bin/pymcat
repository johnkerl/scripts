from __future__ import division # 1/2 = 0.5, not 0.
import re
import sys
from sackmat_m import *

argc = len(sys.argv)
if (argc < 2):
	print >> sys.stderr, "Usage: %s {file name 1} {file name 2}" % (sys.argv[0])
	sys.exit(1)

argi = 1
while (argi < argc):
	A = read_matrix(frac_reader, sys.argv[argi])
	print_matrix(A)
	argi += 1
